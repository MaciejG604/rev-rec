{
  "manifest_version": 2,
  "name": "Gerrit recommendation",
  "description": "This extension recommends code reviewers.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Recommend!"
  },
  "permissions": [
    "activeTab",
    "http://localhost:6060/*"
  ]
}